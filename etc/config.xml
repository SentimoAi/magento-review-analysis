<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <sentimo_review_analysis>
            <sync>
                <enabled>0</enabled>
                <api_base_uri>https://sentimoai.com</api_base_uri>
                <base_uri>https://sentimoai.com</base_uri>
                <channel>magento</channel>
                <post_review_frequency>*/5 * * * *</post_review_frequency>
                <sync_review_frequency>*/5 * * * *</sync_review_frequency>
            </sync>
        </sentimo_review_analysis>
    </default>
</config>
